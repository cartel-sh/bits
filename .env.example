# Database (required even for testing - use dummy value if no DB)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# General
GEMINI_API_KEY=your_gemini_api_key_here

# Sakura Agent
SAKURA_TOKEN=your_sakura_token_here
SAKURA_CLIENT_ID=your_sakura_client_id_here

# Kudasai Agent
KUDASAI_TOKEN=your_kudasai_token_here
KUDASAI_CLIENT_ID=your_kudasai_client_id_here

# Applications Agent
APPLICATIONS_TOKEN=your_applications_bot_token_here
APPLICATIONS_CLIENT_ID=your_applications_client_id_here
APPLICATIONS_PORT=3001
APPLICATIONS_CHANNEL_ID=discord_channel_id_for_applications
WEBHOOK_SECRET=optional_secret_for_webhook_authentication

# Telegram Integration (optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_group_chat_id_here